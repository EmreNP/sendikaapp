{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "types": [
      "node"
    ],
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "../shared/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../shared/**/*.ts"
  ]
}
