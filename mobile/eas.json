{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "internal"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://sendikaapp.web.app"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_PUBLIC_API_BASE_URL": "https://sendikaapp.web.app"
        }
      },
      "ios": {
        "autoIncrement": true,
        "env": {
          "EXPO_PUBLIC_API_BASE_URL": "https://sendikaapp.web.app"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
